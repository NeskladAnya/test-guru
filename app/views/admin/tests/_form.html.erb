<%= form_with model: [:admin, @test], local: true do |form| %>
  <%= render 'devise/shared/error_messages', resource: @test %>
  <p>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </p>

  <p>
    <%= form.label :level %>
    <%= form.number_field :level %>
  </p>

  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>

  <p>
    <%= form.label :time_in_min %>
    <%= form.number_field :time_in_min %>
  </p>

  <p><%= form.submit %></p><br>
<% end %>
