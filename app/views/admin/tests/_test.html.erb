<tbody>
  <tr>
    <td class="text-center"><%= test.title %></td>
    <td class="text-center"><%= test.questions.count %></td>
    <td class="text-center"><%= link_to t('actions.show'), admin_test_path(test) %></td>
    <td class="text-center"><%= link_to t('actions.edit'), edit_admin_test_path(test) %></td>
    <td class="text-center">
      <%= link_to t('actions.delete'),
                  admin_test_path(test),
                  method: :delete,
                  data: {confirm: 'Are you sure?'} %>
    </td>
  </tr>
</tbody>
